page{
  background-color: #f5f5f5;
}
.auto-image{
  width: 100%;
  display: block;
}
.fl{
  float: left;
}
.fr{
  float: right;
}
.clearfix::after{
  content: "";
  display: block;
  clear: both;
}
.box-flex{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-content:space-between;
}
.text-overflow{
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.hide{
  display: none;
}